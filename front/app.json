{
  "expo": {
    "name": "footballNewsManagerFront",
    "description": "Get the news for chosen football teams from most popular leagues.",
    "slug": "footballNewsManager",
    "version": "1.0.0",
    "sdkVersion": "42.0.0",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#FFFFFF",
    "primaryColor": "#006400",
    "icon": "./assets/images/logo.png",
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#006400",
      "iosDisplayInForeground": true,
      "androidMode": "collapse",
      "androidCollapsedTitle": "New news for your teams"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#006400"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png"
      },
      "versionCode": 1
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
