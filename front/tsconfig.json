{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "baseUrl": "./src",
    "paths": {
      "common/*": ["./common/*"],
      "assets/*": ["./assets/*"],
      "api/*": ["./api/*"],
      "hooks/*": ["./hooks/*"],
      "utils/*": ["./utils/*"],
      "views/*": ["./views/*"],
      "mutations/*": ["./api/mutations/*"],
      "queries/*": ["./api/queries/*"],
      "types/*": ["./types/*"],
      "i18n/*": ["./i18n/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
